#include <stdio.h>
void main()
{
  int rem,a;
  int rev=0;
  printf("enter a five digit number\n");
  scanf("%d",&a);
  int org=a;
 while (a!=0)
 {
  rem = a%10;
  rev=(rev*10)+rem;
  a=a/10;
 }
 if(rev==org)
 {
   printf("THE NUMBER IS PALINDRON");
 }
 else
 {
   printf("THE NUMBER IS NOT PALINDRON");
 }
}
