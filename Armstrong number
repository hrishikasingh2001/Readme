#include<stdio.h>
void main()
{
  int rem,result,org;
   result =0;
   int num;
  printf("ENTER ANY NUMBER");
  scanf("%d",&num);
  org = num;
  while (org!=0)
  {
    rem = org%10;
    result = result + (rem*rem*rem);
    org=org/10;
  }
  if(num==result)
  {
    printf("\nTHE NUMBER IS ARMSTRONG");
  }
  else
  {
    printf("\nTHE NUMBER IS NOT ARMSTRONG");
  }
}
