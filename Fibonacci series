#include <stdio.h>
int main() {
    int i, n, t = 0, t1 = 1, next;
    printf("Enter the number of terms: ");
    scanf("%d", &n);
    printf(“\n0”);
    printf("Fibonacci Series: ");

    for (i = 1; i <= n; ++i) {
        printf("\n%d", t1);
        next = t + t1;
        t = t1;
        t1 = next;
    }

    return 0;
}
